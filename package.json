        "react-icons": "^5.5.0",
        "react-redux": "^9.2.0",
        "react-router": "^6.30.1",
        "react-router-dom": "^6.30.1",
        "styled-components": "^6.1.19",
        "tailwindcss": "^4.1.10"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.27.2",
        "@babel/preset-react": "^7.27.1",
        "@testing-library/jest-dom": "^6.6.3",
